  


<div id="engageBox">
  <%= raw @data[1][5] %>
  <div class="engageMedal golden">
     <span id="engageNumber"><%= raw @data[1][4] %></span>
  </div>
</div>
<div id="engageBox">
  <%= raw @data[2][5] %>
  <div class="engageMedal silver">
     <span id="engageNumber"><%= raw @data[2][4] %></span>
  </div>
</div>

<% @data.drop(0).drop(1).drop(2).each do |logo| %>

	<div id="engageBox">
	  <%= raw logo[5] %>
	  <div class="engageMedal">
	     <span id="engageNumber"><%= raw logo[4] %></span>
	  </div>
	</div>
	
<% end %>  
